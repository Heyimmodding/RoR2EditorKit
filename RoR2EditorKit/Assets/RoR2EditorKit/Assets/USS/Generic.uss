.genericHolder {
    border-left-color: rgba(77, 77, 77, 255);
    border-right-color: rgba(77, 77, 77, 255);
    border-top-color: rgba(77, 77, 77, 255);
    border-bottom-color: rgba(77, 77, 77, 255);
    border-left-width: 5px;
    border-right-width: 5px;
    border-top-width: 5px;
    border-bottom-width: 5px;
}

.genericHolderHeader {
    -unity-text-align: middle-center;
    -unity-font-style: normal;
    font-size: 20px;
    background-color: rgba(66, 66, 66, 255);
    border-left-color: rgba(61, 61, 61, 255);
    border-right-color: rgba(61, 61, 61, 255);
    border-top-color: rgba(61, 61, 61, 255);
    border-bottom-color: rgba(61, 61, 61, 255);
    border-left-width: 3px;
    border-right-width: 3px;
    border-top-width: 3px;
    border-bottom-width: 3px;
}

.scriptType {
    -unity-font-style: bold;
}
